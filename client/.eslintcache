/LoginButton.js",[],"/Users/laithhanania/Documents/Projects/Replete/server/client/src/components/CreateCriteriaForm.js",["119"],"import { Form, Field } from \"react-final-form\";\nimport { TextField } from \"final-form-material-ui\";\nimport { Button, Grid } from \"@material-ui/core\";\nimport { postCriteria } from \"../repository/index\";\nimport Proptypes from \"prop-types\";\n\nconst CreateCriteriaForm = ({ criteria, onCancel, onSubmit }) => {\n  const onFormSubmit = async (values) => {\n    const resp = await postCriteria(values);\n    onSubmit();\n  };\n\n  const validate = (values) => {\n    const errors = {};\n    if (!values.label) {\n      errors.label = \"Required\";\n    }\n\n    if (!values.weight) {\n      errors.weight = \"required\";\n    } else if (isNaN(values.weight)) {\n      errors.weight = \"Value must be a number\";\n    } else if (values.weight < 0) {\n      errors.weight = \"Value must be a positive number\";\n    }\n\n    if (criteria.some((criterion) => criterion.label === values.label)) {\n      errors.label = \"Name must be unique\";\n    }\n\n    return errors;\n  };\n\n  return (\n    <div>\n      <Form\n        onSubmit={onFormSubmit}\n        validate={validate}\n        render={({ handleSubmit, submitting, pristine, values, form }) => (\n          <form onSubmit={handleSubmit}>\n            <Grid container alignItems=\"flex-start\" spacing={2}>\n              <Grid item xs={6}>\n                <Field\n                  required\n                  name=\"label\"\n                  component={TextField}\n                  type=\"text\"\n                  label=\"Criteria Name\"\n                />\n              </Grid>\n              <Grid item xs={6}>\n                <Field\n                  InputProps={{ inputProps: { min: 0, max: 10 } }}\n                  required\n                  name=\"weight\"\n                  component={TextField}\n                  type=\"number\"\n                  label=\"Weight\"\n                />\n              </Grid>\n              <Grid item xs={12}>\n                <Field\n                  fullWidth\n                  name=\"description\"\n                  component={TextField}\n                  type=\"text\"\n                  label=\"Description\"\n                />\n              </Grid>\n              <Grid container justify=\"flex-end\">\n                <Grid item>\n                  <Button onClick={onCancel} type=\"button\" color=\"secondary\">\n                    Cancel\n                  </Button>\n                </Grid>\n                <Grid item>\n                  <Button\n                    type=\"button\"\n                    onClick={() => {\n                      form.reset();\n                    }}\n                    disabled={submitting || pristine}\n                  >\n                    Reset\n                  </Button>\n                </Grid>\n                <Grid item>\n                  <Button color=\"primary\" type=\"submit\" disabled={submitting}>\n                    Submit\n                  </Button>\n                </Grid>\n              </Grid>\n            </Grid>\n          </form>\n        )}\n      />\n    </div>\n  );\n};\n\nCreateCriteriaForm.propTypes = {\n  criteria: Proptypes.array,\n  onCancel: Proptypes.func.isRequired,\n  onSubmit: Proptypes.func.isRequired,\n};\n\nexport default CreateCriteriaForm;\n","/Users/laithhanania/Documents/Projects/Replete/server/client/src/components/CreateCriteriaModal.js",[],"/Users/laithhanania/Documents/Projects/Replete/server/client/src/commonComponents/ModalOpenButton.js",[],["120","121"],"/Users/laithhanania/Documents/Projects/Replete/server/client/src/commonComponents/Modal.js",[],"/Users/laithhanania/Documents/Projects/Replete/server/client/src/components/Criteria.js",[],"/Users/laithhanania/Documents/Projects/Replete/server/client/src/repository/index.js",[],"/Users/laithhanania/Documents/Projects/Replete/server/client/src/components/SingleCriteriaCard.js